  xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
  fx:controller="Controller" stylesheets="Steel.css">
  <Tab text="Product Line">
    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
      <GridPane layoutX="27.0" layoutY="64.0" prefHeight="119.0" prefWidth="261.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        </columnConstraints>
        <rowConstraints>
          <RowConstraints maxHeight="44.000013732910155" minHeight="6.399999999999999"
            prefHeight="31.19998779296875" vgrow="SOMETIMES"/>
          <RowConstraints maxHeight="54.4" minHeight="10.0" prefHeight="29.60001220703125"
            vgrow="SOMETIMES"/>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <Label text="Product Name"/>
        <TextField fx:id="addProductName" GridPane.columnIndex="1"/>
        <Label text="Manufacturer" GridPane.rowIndex="1"/>
        <Label text="Item Type" GridPane.rowIndex="2"/>
        <TextField fx:id="addManufacturer" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
        <ChoiceBox fx:id="itemType" prefWidth="150.0" GridPane.columnIndex="1"
          GridPane.rowIndex="2"/>
      </GridPane>
      <Button fx:id="addProductButton" layoutX="116.0" layoutY="261.0" mnemonicParsing="false"
