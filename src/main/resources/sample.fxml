      <TableView layoutX="338.0" layoutY="84.0" prefHeight="265.0" prefWidth="200.0">
        <columns>
          <TableColumn prefWidth="75.0" text="C1"/>
          <TableColumn prefWidth="75.0" text="C2"/>
        </columns>
      </TableView>
      <Label layoutX="393.0" layoutY="46.0" text="Existing Products"/>
    </AnchorPane>
  </Tab>
  <Tab text="Produce">
    <AnchorPane>
      <Label layoutX="109.0" layoutY="30.0" text="Chose Product"/>
      <ListView layoutX="14.0" layoutY="55.0" prefHeight="308.0" prefWidth="298.0"/>
      <Label layoutX="340.0" layoutY="55.0" text="Chose Quantity"/>
      <ComboBox fx:id="addQuantity" layoutX="436.0" layoutY="51.0" prefWidth="150.0"/>
      <Button fx:id="RecordProductButton" layoutX="396.0" layoutY="332.0"
        mnemonicParsing="false" onAction="#recordProduction" text="Record Production"/>
    </AnchorPane>
  </Tab>
  <Tab text="Production Log">
    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
      <TextArea layoutX="74.0" layoutY="33.0" prefHeight="302.0" prefWidth="450.0"/>
    </AnchorPane>
  </Tab>
